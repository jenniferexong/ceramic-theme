  "semanticTokenColors": {
    "variable": "#1e6d56",
    "variable.local": "#616c44",
    "local": "#616c44",
    "property": "#4f6f56",
    "property.declaration": "#4f6f56",
    "class": {
      "foreground": "#796344",
      "fontStyle": "bold"
    },
    "interface": {
      "foreground": "#71714f",
      "fontStyle": "bold"
    },
    "type": {
      "foreground": "#717152",
      "fontStyle": "bold"
    },
    "function": "#1d6870",
    "method": "#1d6870",
    "extensionMethod": "#1d6870",
    "parameter": "#6f6c4a"
  },
  "tokenColors": [
    {
      "name": "Keyword",
      "scope": "keyword.control, keyword.control punctuation, keyword.other",
      "settings": {
        "foreground": "#5b673e",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": [
        "storage",
        "storage.type",
        "keyword.operator.new",
        "entity.name.type"
      ],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#744d43"
      }
    },
    {
      "name": "Module",
      "scope": ["entity.name.type.module"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#848484"
      }
    },
    {
      "name": "namespace",
      "scope": ["entity.name.namespace"],
      "settings": {
        "foreground": "#637b83"
      }
    },
    {
      "name": "Token",
      "scope": ["token", "meta.property-list"],
      "settings": {
        "foreground": "#79808e"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "puncutuation.definition.comment",
        "puncutuation.definition.comment.json",
        "comment.line.double-slash"
      ],
      "settings": {
        "foreground": "#494945",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.regexp punctuation.definition.string",
        "meta.attribute-selector"
      ],
      "settings": {
        "foreground": "#828282"
      }
    },
    {
      "name": "Primitive types",
      "scope": ["support.type.primitive", "keyword.type"],
      "settings": {
        "foreground": "#336278",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Number, boolean",
      "scope": [
        "constant.numeric",
        "constant.language",
        "keyword.other.unit",
        "keyword.operator.quantifier"
      ],
      "settings": {
        "foreground": "#39657f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Scope Resolution",
      "scope": "entity.name.scope-resolution",
      "settings": {
        "foreground": "#5b6f76"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "meta.structure",
        "punctuation.definition.block",
        "meta.brace.square",
        "meta.brace.round",
        "punctuation.definition.parameters",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Optional",
      "scope": ["keyword.operator.optional"],
      "settings": {
        "foreground": "#8c7a6c"
      }
    },
    {
      "name": "pointer access",
      "scope": ["punctuation.separator.pointer-access"],
      "settings": {
        "foreground": "#bfbfbf"
      }
    },
    {
      "name": "variable",
      "scope": ["source variable", "source support.variable"],
      "settings": {
        "foreground": "#5b7883"
      }
    },
    {
      "name": "operator",
      "scope": ["keyword.operator", "punctuation.separator.pointer"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#5b6f76"
      }
    },
    {
      "name": "Constant variable",
      "scope": "source variable.other.constant",
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "CSS constant value",
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#5b6f76"
      }
    },
    {
      "name": "HTML attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#495e48"
      }
    },
    {
      "name": "this",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#a4a4a4"
      }
    },
    {
      "name": "CSS &",
      "scope": "entity.name.tag.reference",
      "settings": {
        "foreground": "#52708b"
      }
    },
    {
      "name": "CSS",
      "scope": ["source.css", "support.type.property-name.css"],
      "settings": {
        "foreground": "#756644"
      }
    },
    {
      "name": "HTML tags",
      "scope": [
        "support.class.component",
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.tsx",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#6b665c"
      }
    }
  ]
}
