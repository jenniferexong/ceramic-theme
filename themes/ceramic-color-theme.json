  "semanticTokenColors": {
    "variable": "#5c7f8a",
    "variable.local": "#5c7f8a",
    "property": "#698c76",
    "property.declaration": "#698c76",
    "class": {
      "foreground": "#a08c71",
      "fontStyle": "bold"
    },
    "interface": {
      "foreground": "#9f9f9f",
      "fontStyle": "bold"
    },
    "function": "#407c80",
    "method": "#407c80",
    "parameter": "#828066",
    "type": {
      "foreground": "#9f9f9f",
      "fontStyle": "bold"
    }
  },
  "tokenColors": [
    {
      "name": "Keyword",
      "scope": "keyword.control, keyword.control punctuation, keyword.other",
      "settings": {
        "foreground": "#8c8f78",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage", "storage.type", "keyword.operator.new"],
      "settings": {
        "fontStyle": "bold",
        "foreground": "#855959"
      }
    },
    {
      "name": "Module",
      "scope": ["entity.name.type.module", "variable.other.object"],
      "settings": {
        "foreground": "#647f88"
      }
    },
    {
      "name": "namespace",
      "scope": ["entity.name.namespace"],
      "settings": {
        "foreground": "#637b83"
      }
    },
    {
      "name": "Token",
      "scope": ["token", "meta.property-list"],
      "settings": {
        "foreground": "#98A6BF"
      }
    },
    {
      "name": "Comment",
      "scope": [
        "comment",
        "puncutuation.definition.comment",
        "puncutuation.definition.comment.json",
        "comment.line.double-slash"
      ],
      "settings": {
        "foreground": "#494945",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": [
        "string",
        "punctuation.definition.string",
        "string.regexp punctuation.definition.string",
        "meta.attribute-selector"
      ],
      "settings": {
        "foreground": "#a5a5a5"
      }
    },
    {
      "name": "Primitive types",
      "scope": ["support.type.primitive"],
      "settings": {
        "foreground": "#507788",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Number, boolean",
      "scope": [
        "constant.numeric",
        "constant.language",
        "keyword.other.unit",
        "keyword.operator.quantifier"
      ],
      "settings": {
        "foreground": "#4b839b",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Scope Resolution",
      "scope": "entity.name.scope-resolution",
      "settings": {
        "foreground": "#5b6f76"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "meta.structure",
        "punctuation.definition.block",
        "meta.brace.square",
        "meta.brace.round",
        "punctuation.definition.parameters",
        "keyword.operator.logical"
      ],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "pointer access",
      "scope": ["punctuation.separator.pointer-access"],
      "settings": {
        "foreground": "#bfbfbf"
      }
    },
    {
      "name": "variable",
      "scope": ["source variable", "source support.variable"],
      "settings": {
        "foreground": "#7d949d"
      }
    },
    {
      "name": "operator",
      "scope": ["keyword.operator", "punctuation.separator.pointer"],
      "settings": {
        "foreground": "#888888"
      }
    },
    {
      "name": "Parameter",
      "scope": "variable.parameter",
      "settings": {
        "foreground": "#5b6f76"
      }
    },
    {
      "name": "Constant variable",
      "scope": "source variable.other.constant",
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "CSS constant value",
      "scope": "support.constant.property-value",
      "settings": {
        "foreground": "#989898"
      }
    },
    {
      "name": "User-defined constant",
      "scope": "constant.character, constant.other",
      "settings": {
        "foreground": "#5b6f76"
      }
    },
    {
      "name": "HTML attribute",
      "scope": "entity.other.attribute-name",
      "settings": {
        "foreground": "#556353"
      }
    },
    {
      "name": "this",
      "scope": "variable.language.this",
      "settings": {
        "foreground": "#a4a4a4"
      }
    },
    {
      "name": "CSS &",
      "scope": "entity.name.tag.reference",
      "settings": {
        "foreground": "#52708b"
      }
    },
    {
      "name": "CSS",
      "scope": ["source.css", "support.type.property-name.css"],
      "settings": {
        "foreground": "#686868"
      }
    },
    {
      "name": "HTML tags",
      "scope": [
        "support.class.component",
        "punctuation.definition.tag.begin.tsx",
        "punctuation.definition.tag.end.tsx",
        "entity.name.tag"
      ],
      "settings": {
        "foreground": "#686868"
      }
    }
  ]
}
